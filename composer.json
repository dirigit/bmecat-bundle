{
    "name": "se/bmecat-bundle",
    "type": "symfony-bundle",
    "description": "This bundle integrates the bmecat library into Symfony2. Allows to build BMEcat documents from PHP and converts them to xml",
    "keywords": ["bmecat", "symfony2", "bundle", "document", "serialization", "xml"],
    "homepage": "https://github.com/sveneisenschmidt/bmecat-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Sven Eisenschmidt",
            "email": "sven.eisenschmidt@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "se/bmecat": "~0.1",
        "symfony/framework-bundle": "~2.1"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*"
    },
    "autoload": {
        "psr-0": {
            "SE\\Bundle\\BMEcat": "src/"
        }
    }
}